{
    "name": "pixelports",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "npm run check-env && next dev --turbopack -p 3000",
        "check-env": "tsx scripts/check-env.ts",
        "build": "npm run check-env && next build --turbopack",
        "start": "next start -p 3001",
        "format": "prettier --write .",
        "format:check": "prettier --check .",
        "test:ui": "playwright test",
        "test:ui:update": "playwright test --update-snapshots",
        "supabase:start": "supabase start",
        "supabase:stop": "supabase stop",
        "createsnapshot": "tsx scripts/restore.ts",
        "snapshot:create": "tsx scripts/snapshot.ts",
        "restoresnapshot": "tsx scripts/restore.ts",
        "snapshot:restore": "tsx scripts/restore.ts",
        "snapshots": "tsx scripts/list-snapshots.ts",
        "verify-setup": "tsx scripts/verify-setup.ts"
    },
    "dependencies": {
        "@hookform/resolvers": "^5.2.2",
        "@sentry/nextjs": "^10.26.0",
        "@supabase/ssr": "^0.8.0",
        "@supabase/supabase-js": "^2.86.0",
        "@tanstack/react-query": "^5.90.11",
        "file-type": "^21.1.1",
        "image-size": "^2.0.2",
        "lucide-react": "^0.555.0",
        "mixpanel-browser": "^2.72.0",
        "next": "16.0.6",
        "react": "19.2.0",
        "react-dom": "19.2.0",
        "react-hook-form": "^7.67.0",
        "supabase": "^2.63.1",
        "zod": "^4.1.13",
        "dotenv": "^17.2.3"
    },
    "devDependencies": {
        "@playwright/test": "^1.57.0",
        "@tailwindcss/postcss": "^4.1.17",
        "@types/fs-extra": "^11.0.4",
        "@types/node": "^24.10.1",
        "@types/react": "^19.2.7",
        "@types/react-dom": "^19.2.3",
        "execa": "^9.6.1",
        "fs-extra": "^11.2.0",
        "prettier": "^3.7.3",
        "tailwindcss": "^4.1.17",
        "tsx": "^4.21.0",
        "typescript": "^5.9.3",
        "typescript-eslint": "^8.48.0"
    }
